{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "VT_API_KEY": "2c275dc00ce1271ad55f7cffb63fb88ec15640f37ec619554d56b8318f4a64d8",
    "SECRET_KEY": "your-secret-key-here"
  }
}